<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.modul_praktikum">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Modul_Praktikum">
        <activity
            android:name=".UpdatePenggunaActivity"
            android:exported="false" />
        <activity
            android:name=".ListPenggunaActivity"
            android:exported="false" />
        <activity
            android:name=".HalamanUtama"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Modul_Praktikum.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".HalamanHasil" />
    </application>

</manifest>